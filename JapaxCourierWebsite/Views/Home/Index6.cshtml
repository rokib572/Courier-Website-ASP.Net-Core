            ball1.draw(70, 0);
            ball2.draw(20, 200);
            //ball3.draw(50, 30);
            ball4.draw(50, 20);


            let allIconsDoms = document.querySelectorAll('.around-mobile-circled-icons span');
            let startAngle = 135;
            let endAngle = 405; // 45deg
            let eachAngle = (endAngle - startAngle) / (allIconsDoms.length - 1);
            let radius = document.body.clientWidth * 13/100;

           @* if (document.body.clientWidth > 1440) {
                radius = 220;
            } else {
                radius = 170;
            }*@
            let $circleSvgAnimationDiv = document.querySelector('#circle-svg-animation');
            let $circlePathAnimationDiv = document.querySelector('#circle-svg-animation .circle');
             // same as half width of .around-mobile-circled-icons
            document.querySelector('.around-mobile-circled-icons').setAttribute("style", `height: ${2 * radius}px; width: ${2 * radius}px`);
            $circleSvgAnimationDiv.setAttribute("style", `height: ${2 * radius}px; width: ${2 * radius}px`);

            let strokeWidth = 1;
            $circleSvgAnimationDiv.setAttribute("viewBox", `0 0 ${2 * (radius + strokeWidth)} ${2 * (radius + strokeWidth)}`);
            $circlePathAnimationDiv.setAttribute("stroke-width", strokeWidth)
            $circlePathAnimationDiv.setAttribute("transform", `rotate(180,${radius},${radius})`)
            $circlePathAnimationDiv.setAttribute("cx", radius - strokeWidth)
            $circlePathAnimationDiv.setAttribute("cy", radius - strokeWidth)
            $circlePathAnimationDiv.setAttribute("r", radius)

            allIconsDoms.forEach((el, i) => {

                let radian_value = (startAngle + eachAngle * i) * Math.PI / 180
                let X = radius * Math.cos(radian_value)
                let Y = radius * Math.sin(radian_value)

                el.setAttribute("style", `transform: translate(${X}px, ${Y}px)`);
            });

            const circle = document.querySelector('#circle-svg-animation .circle');
            const angle_increment = 10;
            const interval = 120;

            $('.banner-slider').on('beforeChange', function (event, slick, currentSlide, nextSlide) {
                if (currentSlide == 4) return
                let angle = currentSlide * 135;
                let constAngle = currentSlide * 135;
                if (constAngle >= 535) {
                    angle = 0;
                    constAngle = 0;
                }

                window.timer = window.setInterval(function () {
                    circle.setAttribute("stroke-dasharray", angle + ", 20000");

                    if (angle >= constAngle + 120) {
                        window.clearInterval(window.timer);
                    }
                    angle += angle_increment;
                }.bind(this), interval);

            })

            $('.banner-slider').on('afterChange', function (event, slick, currentSlide, nextSlide) {
                if (currentSlide == 0) $('.around-mobile-circled-icons span').removeClass("active-banner-icon-slide");
                $('.around-mobile-circled-icons span').removeClass("active-banner-icon-current-slide");
                $(`#icon-${currentSlide}`).addClass("active-banner-icon-slide active-banner-icon-current-slide");
