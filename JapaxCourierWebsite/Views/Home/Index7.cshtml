                            textIndex++;
                        } else {
                            setTimeout(function () {
                                deleting(index, text);
                                $('#banner-top-img-' + index).fadeOut(500);
                            }, 4000);
                            clearInterval(tmp);
                        }
                    }, 100);
                }

                function deleting(index, text) {
                    var textIndex = text[index].length;
                    var tmp = setInterval(function () {
                        if (textIndex + 1 > 0) {
